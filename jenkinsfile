pipeline
{
    agent any
    stages
    {
        stage('print')
        {
               steps
               {
                        sh 'echo Hi'    
                    
               } 
            stage ('code compile' )
                {
                    steps {  
                        withMaven(jdk: 'JDK_HOME', maven: 'MAVEN_HOME') 
                        
                        { sh 'mvn compile'}
   }}

        }
    }
}


